  <link rel="stylesheet" href="/css/style.css">
  <link rel="stylesheet" href="/css/productadd.css">
  <title><%=pageTitle%></title>
</head>
<body>
<div id="overlay" onclick="overlayOff()"></div> <!--overlay entire page whenever needed-->
  <header>
    <%- include('partials/Navbars/nav1.ejs') %>
      <%- include('partials/Navbars/nav2.ejs') %>
  </header>
  <main>
    <div class="row">
      <div class="col-3">
    
    
      </div>
    <div class="col-9">
      <form action="/add-product" method="POST">
        <h1>Fill in Product Details</h1>
        <div class="input-container">
          <input name="name" id="name1" class="input" type="text" pattern=".+" required />
          <label class="label" for="name1">Product Name</label>
        </div>
        <div class="input-container">
          <input name="price" id="name2" class="input" type="text" pattern=".+" required />
          <label class="label" for="name2">Price</label>
        </div>
        <div class="input-container">
          <input name="discount" id="name3" class="input" type="text" pattern=".+" required />
          <label class="label" for="name3">Discount</label>
        </div>
        <div class="input-container">
          <input name="imageurl" id="name4" class="input" type="text" pattern=".+" required />
          <label class="label" for="name4">Image URL</label>
        </div>
        <div class="input-container">
          <input name="description" id="name5" class="input" type="text" pattern=".+" required />
          <label class="label" for="name5">Description</label>
        </div>
        <button type="submit" class="btn btn-outline-info btn-fr">Submit</button>
      </form>
    
    
    </div>
    
    </div>
    
    
    </main>


  <%- include('partials/Navbars/footer.ejs') %>

  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.6/dist/umd/popper.min.js"
