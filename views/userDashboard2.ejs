                        <option selected>India</option>
                        <option>Other</option>
                      </select>
                    </div>
                </div>
                <div class="d-flex pt-5"><button type="submit" class="btn btn-primary me-3">Save Changes</button> <button type="reset" class="btn btn-link">Cancel</button></div>
